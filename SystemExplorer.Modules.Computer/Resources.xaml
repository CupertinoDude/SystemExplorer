<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sf="http://schemas.syncfusion.com/wpf"
                    xmlns:view="clr-namespace:SystemExplorer.Modules.Computer.Views"
                    xmlns:vm="clr-namespace:SystemExplorer.Modules.Computer.ViewModels">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/SystemExplorer.Core;component/Resources/Common.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <DataTemplate DataType="{x:Type vm:ComputerViewModel}">
        <view:ComputerView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EnvironmentVariablesViewModel}">
        <view:EnvironmentVariablesView />
    </DataTemplate>

</ResourceDictionary>