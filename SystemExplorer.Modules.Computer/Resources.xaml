<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:SystemExplorer.Modules.Computer.Views"
                    xmlns:local="clr-namespace:SystemExplorer.Modules.Computer">
    <DataTemplate DataType="{x:Type local:ComputerViewModel}">
        <view:ComputerView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:EnvironmentVariablesViewModel}">
        <view:EnvironmentVariablesView />
    </DataTemplate>
</ResourceDictionary>